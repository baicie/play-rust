{
  "$schema": "./config.schema.json",
  "job_name": "mysql_to_postgres",
  "source": {
    "name": "mysql_source",
    "connector_type": "mysql",
    "properties": {
      "url": "mysql://root:password@localhost:3306/test",
      "table": "source_table",
      "batch_size": 1000
    }
  },
  "sink": {
    "name": "postgres_sink",
    "connector_type": "postgres",
    "properties": {
      "url": "postgres://postgres:password@localhost:5432/test",
      "table": "target_table",
      "max_connections": 5
    }
  }
}
